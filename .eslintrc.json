{
   

    "parser": "babel-eslint",
    
        "extends": [
          "plugin:prettier/recommended"
        ],
      
        "plugins": ["prettier"],
      
    "rules": {
        "prettier/prettier": ["error", {"singleQuote": true, "parser": "flow"}],
        "quotes": [2, "single", "avoid-escape"],
        "semi": ["error", "always"],
        "constructor-super": 2,
        "for-direction": 2,
        "getter-return": 2,
        "no-async-promise-executor": 2,
        "no-case-declarations": 2,
        "no-class-assign": 2,
        "no-compare-neg-zero": 2,
        "no-cond-assign": 2,
        "no-const-assign": 2,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-dupe-args": 2,
        "no-dupe-class-members": 2,
        "no-dupe-else-if": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-empty-pattern": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-func-assign": 2,
        "no-import-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-misleading-character-class": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-new-symbol": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-prototype-builtins": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-self-assign": 2,
        "no-setter-return": 2,
        "no-shadow-restricted-names": 2,
        "no-sparse-arrays": 2,
        "no-this-before-super": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "no-unsafe-finally": 2,
        "no-unsafe-negation": 2,
        "no-unused-labels": 2,
        "no-useless-catch": 2,
        "no-useless-escape": 2,
        "no-with": 2,
        "require-yield": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "array-bracket-spacing": 2,
        "arrow-body-style": 2,
        "arrow-parens": 2,
        "arrow-spacing": 2,
        "block-scoped-var": 2,
        "brace-style": 2,
        "callback-return": 2,
        "camelcase": 2,
        "comma-dangle": 2,
        "curly": 2,
        "dot-notation": 2,
        "func-name-matching": 2,
        "function-paren-newline": 2,
        "generator-star-spacing": 2,
        "key-spacing": 2,
        "keyword-spacing": 2,
        "line-comment-position": 2,
        "no-constructor-return": 2,
        "no-multi-spaces": 2
    },
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "shared-node-browser": true,
        "worker": false,
        "amd": false,
        "mocha": true,
        "jasmine": false,
        "jest": true,
        "phantomjs": false,
        "jquery": true,
        "qunit": false,
        "prototypejs": true,
        "shelljs": false,
        "meteor": false,
        "mongo": false,
        "protractor": false,
        "applescript": false,
        "nashorn": false,
        "serviceworker": false,
        "atomtest": false,
        "embertest": false,
        "webextensions": false,
        "es6": true,
        "es2017": true,
        "es2020": true,
        "greasemonkey": false
    }
}